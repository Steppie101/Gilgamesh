FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      faSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         bdf2;
}

gradSchemes
{
    default         Gauss linear; // Grad: Us, surfaceTension
//    grad(testAF)    leastSquares;
}

divSchemes
{
    default         Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear; // Interpolate: Us
}

snGradSchemes
{
    default         none;
}

