FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/fluid";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


// air properties taken from https://www.engineeringtoolbox.com/air-properties-d_156.html

// Density - Dimensions: kg/m3
rho             rho [1 -3 0 0 0 0 0] 1.2;

// Dynamic viscosity - Dimensions: Pa s
mu              mu [ 1 -1 -1 0 0 0 0 ] 1.8e-5;

// Specific heat - Dimensions: J/kg-K
cp              cp  [0 2 -2 -1 0 0 0] 1005;

// Thermal conductivity - Dimensions: W/m-K
k            	k   [1 1 -3 -1 0 0 0] 0.025;

// Molecular diffusion coefficient - Dimensions: m2/s
DCO2            DCO2 [0 2 -1 0 0 0 0] 1.6e-5;
DH2O            DH2O [0 2 -1 0 0 0 0] 2.4e-5;

