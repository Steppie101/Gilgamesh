FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      coupledSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// rhoPimpleFluid zonder transportTemperature

regions
(
    (fluid (icoFluid transportTemperature) )
    //(fluid (rhoPimpleFluid) )
    (solid (conductTemperature) )
);


partitionedCoupling
{
    maxCoupleIter 6;
}

DNA
{
   
    T
    {
        maxCoupleIter 20;

        residualControl
        {
            maxJumpRes 1e-07;
            outputJumpResField no;
            maxFluxRes 1e-07;
            outputFluxResField yes;
        }
    } 
    CO2
    {
        maxCoupleIter 20;

        residualControl
        {
            maxJumpRes 1e-06;
            outputJumpResField no;
            maxFluxRes 1e-06;
            outputFluxResField yes;
        }
    }
     H2O
     {
         maxCoupleIter 20;

         residualControl
         {
             maxJumpRes 1e-06;
             outputJumpResField no;
             maxFluxRes 1e-06;
             outputFluxResField yes;
         }
     }
}

