FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 0.025;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0.025;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.025;
    }
    sides
    {
        type            calculated;
        value           uniform 0.025;
    }
    fluid_to_solid
    {
        type            calculated;
        value           uniform 0.025;
    }
}


